{
    "manifest_version": 3,
    "name": "Hadith Tab",
    "description": "Hadith Tab Is A New Tab Page That Represent AHadith Of The Prophet Peace Be Upon Him On Each Tab",
    "version": "1.0",
    

    "background": {
        "service_worker": "background.js"
    },

    "chrome_url_overrides": {
        "newtab": "HadithTab.html"
        },

        "permissions": [
            "tabs",
            "storage",
            "activeTab"
        ],

    "icons": {
        "16": "/Images/Icons/D16.png",
        "32": "/Images/Icons/D32.png",
        "48": "/Images/Icons/D48.png",
        "128": "/Images/Icons/D128.png"
    }






}